  (func $one (;3;) (type 0) (result i32)
    i64.const 4
    i64.const 0
    call $finite_wasm_stack
    i64.const 1
    call $finite_wasm_gas
    i32.const 1
  )
  (func $two (;4;) (type 0) (result i32)
    i64.const 4
    i64.const 0
    call $finite_wasm_stack
    i64.const 1
    call $finite_wasm_gas
    i32.const 2
  )
  (func (;5;) (type 1) (param i32 i32 i32)
    i64.const 12
    i64.const 3
    call $finite_wasm_stack
    i64.const 3
    call $finite_wasm_gas
    local.get 0
    local.get 1
    local.get 2
    i64.const 1
    call $finite_wasm_gas
    table.copy
  )
  (func (;6;) (type 2) (param i32) (result i32)
    i64.const 8
    i64.const 1
    call $finite_wasm_stack
