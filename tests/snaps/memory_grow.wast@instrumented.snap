  (func (;3;) (type 0) (param i32) (result i32)
    block (result i32) ;; label = @1
      i64.const 4
      i64.const 1
      call 1
      i64.const 1
      call 0
      local.get 0
      i64.const 1
      call 0
      memory.grow
    end
    i64.const 4
    i64.const 1
    call 2
  )
  (func (;4;) (type 1) (param i32 i32) (result i32)
    (local i32)
    block (result i32) ;; label = @1
      i64.const 8
      i64.const 3
      call 1
      i64.const 3
      call 0
      i32.const 1
      local.set 2
      block ;; label = @2
        loop ;; label = @3
          i64.const 2
          call 0
          local.get 0
          i64.const 1
          call 0
          i32.load8_u
          i64.const 5
          call 0
          local.set 2
          local.get 2
          i32.const 0
          i32.ne
          br_if 1 (;@2;)
          i64.const 4
          call 0
          local.get 0
          local.get 1
          i32.ge_u
          br_if 1 (;@2;)
          i64.const 8
          call 0
          local.get 0
          i32.const 1
          i32.add
          local.set 0
          local.get 0
          local.get 1
          i32.le_u
          br_if 0 (;@3;)
        end
      end
      i64.const 1
      call 0
      local.get 2
    end
    i64.const 8
    i64.const 3
    call 2
  )
